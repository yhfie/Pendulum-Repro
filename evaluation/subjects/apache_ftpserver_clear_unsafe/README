cd /Users/yannic/repositories/QuantiFuzz/evaluation/example_PWCheck_unsafe


#1 Instrument bytecode:
java -cp "../../tool/instrumentor/build/libs/kelinci.jar:lib/*" edu.cmu.sv.kelinci.instrumentor.Instrumentor -mode LABELS -i ./bin/ -o ./bin-instr -skipmain


#2 Start Kelinci server:
java -cp ./bin-instr/ edu.cmu.sv.kelinci.Kelinci -K 30 Driver @@


#3 Test execution with interface program:
../../tool/fuzzerside/interface -K 30 in_dir/example.txt


#4 Start Fuzzing
AFL_I_DONT_CARE_ABOUT_MISSING_CRASHES=1 AFL_SKIP_CPUFREQ=1 ../../tool/afl-2.51b-wca/afl-fuzz -i in_dir -o fuzzer-out -c quantify -K 30 -S afl -t 999999999 ../../tool/fuzzerside/interface -K 30 @@